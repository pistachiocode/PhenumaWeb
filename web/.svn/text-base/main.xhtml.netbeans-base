            function ejemplo1()
            {          
                $("textarea[id = 'queryform:inputtxtarea']").val("OTC")              
                $("input[id='queryform:inputtype:0']").click()         
                $("button[id='queryform:buildnetworkbtn']").click()
                
                                
                pbAjax.cancel();
                pbAjax.start();
            }
            
