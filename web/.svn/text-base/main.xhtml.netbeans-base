            {          
                $("textarea[id = 'queryform:inputtxtarea']").val("HP:0001691, HP:0003116, HP:0001953")              
                $("input[id='queryform:inputtype:3']").click()         
                $("button[id='queryform:buildnetworkbtn']").click()
                                
                pbAjax.cancel();
                pbAjax.start();
            }
