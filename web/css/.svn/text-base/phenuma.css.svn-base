}


/**
 * Filter panel styles
 */
 
.filterdivcell{
    display:table-cell; 
    width: 20px; 
    vertical-align:middle;
}

.filter_label{
   font-weight: bold; 
   font-size: 1.1em;
}


.filter_label:hover{
    font-weight: bold; 
   font-size: 1em;
    color: #F39814;
}


/**
 * Primefaces components
 */
 
.ui-widget {
    font-size: 12px;

}

.ui-widget .ui-widget {
    font-size: 10px;
}            

.ui-layout, .ui-layout-doc, .ui-layout-unit, .ui-layout-wrap, .ui-layout-bd, .ui-layout-hd{
    border: none;
    font-size: 12px;
    background-color: #BDBDBD;
}

.ui-layout-north .ui-layout-unit-content {
    padding: 0 0 0 0;
    margin: 0 0 0 ;    
    background: none;
    border:none;
}



.ui-tabs .ui-tabs-panel{ 
    border-width: 0; 
    padding: 0 0; 
    background: none; 
    border-bottom-color: #FFF;
    font-size: 1em;
}


/**
 * Accordion style
 */
 
.ui-accordion .ui-accordion-header { 
    font-size: 1em;
}

.ui-accordion .ui-accordion-content { 
    padding: 0 0; 
    border-top: 1; 
    margin-top: -2px; 
    overflow: auto; 
    border: 0px;
    padding:  0 0;
    font-size: 1em;
}


 
 .ui-fieldset-legend {
    font-size: 1em;
 }
 


.ui-slider-horizontal .ui-slider-handle {
    left: 0;
    margin-bottom: -41px;
    margin-left: 0;
    height: 10px;
    width: 10px;
    background-color: blue;
}
.ui-layout-center {                    
    overflow:hidden !important;
}

.ui-layout-center .ui-layout-unit-content {
    overflow:hidden !important;
}

.ui-layout-unit {
     overflow:hidden !important;
}

.ui-messages-info, .ui-messages-warn, .ui-messages-error, .ui-messages-fatal {
    border: 2px solid;
    border-color: #393A3A;
    margin: 5px 5px 5px 5px;
    padding: 10px 5px 5px 5px;
    font-size: 14px;
    color: #393A3A;
    width: 96%;
    height:auto;
}



.animated .ui-progressbar-value {
   border:0px;
   background-image: url("../resources/img/ajax-loader-animated.gif"); 

}



.ui-state-active {
    background: #393A3A;
    color: white;
}


.ui-state-default {
}

